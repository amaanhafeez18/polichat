{
    "compilerOptions": {
        "declaration": true,
        "forceConsistentCasingInFileNames": true,
        "incremental": true,
        "module": "commonjs",
        "target":"ES6",
        "outDir": "./lib",
        "rootDir": "./src",
        "sourceMap": true,
        "strict": true,
        "tsBuildInfoFile": "./lib/.tsbuildinfo",
        "esModuleInterop": true,
        "maxNodeModuleJsDepth": 10,
        "skipLibCheck": true
    }
    
}
